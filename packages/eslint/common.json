{
  "rules": {
    "keyword-spacing": [
      "error"
    ],
    "array-bracket-spacing": [
      "error",
      "never"
    ],
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "no-undef": "off",
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "eol-last": [
      "error",
      "always"
    ],
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-trailing-spaces": "error"
  }
}
